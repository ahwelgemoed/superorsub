{
  "version": "2.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "build:all": "npm run build && cd example && npm run build",
    "demo": "cd example && npm run preview"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "superorsub",
  "description": "A package to Type Super or Subscript in HTML inputs with minimal setup.",
  "author": "ahwelgemoed",
  "module": "dist/superorsub.esm.js",
  "keywords": [
    "subscript",
    "superscript",
    "chemical",
    "chemical formula",
    "HTML Input"
  ],
  "size-limit": [
    {
      "path": "dist/superorsub.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/superorsub.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
